import pandas as pd
from email_verifier import verify_email
from lead_scoring import score_leads

# Sample data
data = pd.read_csv("sample_leads.csv")

# Verify emails
data["verified"] = data["email"].apply(verify_email)

# Score leads
data["score"] = data.apply(score_leads, axis=1)

